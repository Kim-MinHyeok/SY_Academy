package homework;

public class Page130_15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] a = new int[3][5];
		int ran = (int)(Math.random() * 4);
		
		for(int i=0; i<a.length;i++) {
			for(int j=0;j<a[i].length;j++) {
				a[i][j] = 0;
			}
		}

		for(int i=0; i<a.length;i++) {
			for(int j=0;j<a[i].length;j++) {
				a[i][j] = 0;
			}
		}
		
		System.out.println("");
	}

}
