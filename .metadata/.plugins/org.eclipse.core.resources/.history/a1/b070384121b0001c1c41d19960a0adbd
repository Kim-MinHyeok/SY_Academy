package day0330;

public class Page128_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String temp = "";
		int num = 0;

		for (int i = 1; i <= 50; i++) {
			temp = Integer.toString(i);
			
			if (temp.contains("3")) {
				for (int j = 0; j < temp.length(); j++) {
					if (temp.charAt(j) == '3' || temp.charAt(j) == '6' || temp.charAt(j) == '9') {
						num++;
						System.out.print("Â¦");
					} 
				}
				System.out.print(" ");
			} else {
				System.out.print(i + " ");
			}		
			
			temp = "";
			num = 0;	
		}
	}
}
