package day0401;

public class MethodTest07 {
	public static int comparison (int a, int b) {
		int r;
		if(a > b) {
			r = a;
		} else {
			r = b;
		}
		
		return r;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		comparison(1,3);
		comparison(3,2);
		comparison(5,9);
	}

}
