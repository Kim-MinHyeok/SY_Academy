package day0401;

public class MethodTest06 {
	public static void comparison (int a, int b) {
		if(a > b) {
			System.out.println(a + "가 더 크다");
		} else {
			System.out.println(b + "가 더 크다");
		}
		return;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		comparison(1,3);
		comparison(3,2);
		comparison(5,9);
	}

}
