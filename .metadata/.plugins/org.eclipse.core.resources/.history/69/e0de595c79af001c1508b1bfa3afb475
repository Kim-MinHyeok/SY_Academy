package day0328;

public class HomeWork {

 public static void printString(int n) {
  String []kor = {"", "ÀÏ", "ÀÌ", "»ï", "»ç", "¿À", "À°", "Ä¥", "ÆÈ", "±¸"};
  System.out.print( kor[n] );
 }

 public static void print100(int n100) {
	if( n100 >= 2) {
		printString(n100);
		System.out.print("¹é");     
	} else if( n100 == 1)
	  System.out.print("¹é");   
	}

 public static void print10(int n10) {
	if(n10 >= 2){
		printString(n10);
		System.out.print("½Ê");
    }
	else if(n10==1)
    	System.out.print("½Ê"); 
 }


