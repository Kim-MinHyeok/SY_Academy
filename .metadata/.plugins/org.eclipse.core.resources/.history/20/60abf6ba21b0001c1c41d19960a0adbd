package day0330;

public class Page128_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String temp = "";
		int num = 0;

		for (int i = 1; i <= 50; i++) {
			temp = Integer.toString(i); // 정수를 문자열로 변환해서 임시 변수에 저장
			
			if (temp.contains("3") || temp.contains("6") || temp.contains("9")) { // 임시 변수에 3, 6, 9 포함인지 검사
				for (int j = 0; j < temp.length(); j++) { // 포함됐을 경우 임시변수의 자릿수만큼 반복해서 3, 6, 9가 포함된 수만큼 "짝"을 출력한다
					if (temp.charAt(j) == '3' || temp.charAt(j) == '6' || temp.charAt(j) == '9') {
						num++;
						System.out.print("짝");
					} 
				}
				System.out.print(" ");
			} else {
				System.out.print(i + " ");
			}		
			
			temp = "";
			num = 0;	
		}
	}
}
