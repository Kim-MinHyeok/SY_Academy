package day0401;

public class MethodTest12 {
	public static boolean getAvg (int[] num, int[] num2) {
		boolean same;
		
		if(num.length == num2.length) {
			same = true;
		}
		for (int i=0;i<num.length;i++) {
			avg +=num[i];
		}
		
		return same;
	}

