import java.io.*;
class HomeWork {

 public static void printString(int n) {
  String []kor = {"", "일", "이", "삼", "사", "오", "육", "칠", "팔", "구"};
  System.out.print( kor[n] );
 }

 public static void print100(int n100) {
	if( n100 >= 2) {
		printString(n100);
		System.out.print("백");     
	} else if( n100 == 1)
	  System.out.print("백");   
	}

 public static void print10(int n10) {
	if(n10 >= 2){
		printString(n10);
		System.out.print("십");
    }
	else if(n10==1)
    	System.out.print("십"); 
 }


 public static void main(String []args) {
	int n ;
	int n1, n10, n100;

	try {
		DataInputStream dis = new DataInputStream(System.in);
	while(true) {
		System.out.print("0~999사이의 정수를 입력하시오=>");
    
		n = Integer.parseInt(dis.readLine());
    
    if( n <0 || n >999) {
    	System.out.println("프로그램을 종료합니다.");
    	break;
    }

    if( n == 0)
    {
     System.out.println("영");
     continue;
    }

    n100 = n / 100;
    n = n %100;
    n10 = n / 10;
    n1 = n %10;
    
    print100(n100);
    print10(n10);          
    printString(n1);
    System.out.println();
   }
  }catch(Exception e){}
 }
}